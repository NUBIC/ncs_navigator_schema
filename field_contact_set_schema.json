{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "description": "Field contact set",
    "properties": {
        "contacts": {
            "description": "Contacts in the contact set",
            "items": {
                "properties": {
                    "contact_id": {
                        "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                    },
                    "end_date": {
                        "format": "date-time",
                        "required": true,
                        "type": "string"
                    },
                    "events": {
                        "items": {
                            "properties": {
                                "event_id": {
                                    "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                                },
                                "instruments": {
                                    "items": {
                                        "properties": {
                                            "instrument_id": {
                                                "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                                            },
                                            "instrument_template_id": {
                                                "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                                            },
                                            "name": {
                                                "required": true,
                                                "type": "string"
                                            },
                                            "response_set": {
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "minItems": 1,
                                    "required": true,
                                    "type": "array"
                                },
                                "name": {
                                    "required": true,
                                    "type": "string"
                                }
                            }
                        },
                        "minItems": 1,
                        "required": true,
                        "type": "array"
                    },
                    "location": {
                        "properties": {
                            "address": {
                                "properties": {
                                    "address_id": {
                                        "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                                    },
                                    "city": {
                                        "type": "string"
                                    },
                                    "state": {
                                        "type": "string"
                                    },
                                    "street": {
                                        "type": "string"
                                    },
                                    "zip_code": {
                                        "type": "string"
                                    }
                                },
                                "required": true,
                                "type": "object"
                            },
                            "code": {
                                "required": true,
                                "type": "string"
                            },
                            "details": {
                                "type": "string"
                            },
                            "other": {
                                "type": "string"
                            }
                        },
                        "required": true,
                        "type": "object"
                    },
                    "person": {
                        "properties": {
                            "cell_phone": {
                                "format": "phone",
                                "type": "string"
                            },
                            "email": {
                                "type": "string"
                            },
                            "home_phone": {
                                "format": "phone",
                                "type": "string"
                            },
                            "name": {
                                "required": true,
                                "type": "string"
                            },
                            "person_id": {
                                "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                            }
                        },
                        "required": true,
                        "type": "object"
                    },
                    "start_date": {
                        "format": "date-time",
                        "required": true,
                        "type": "string"
                    },
                    "type": {
                        "required": true,
                        "type": "string"
                    }
                }
            },
            "required": true,
            "type": "array"
        }
    }
}
