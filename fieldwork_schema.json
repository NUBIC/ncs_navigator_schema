{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "description": "Field contact set",
    "properties": {
        "contacts": {
            "description": "Contacts in the contact set",
            "items": {
                "extends": {
                    "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/versioned_entity_schema.json#"
                },
                "properties": {
                    "contact_date": {
                        "format": "date",
                        "required": true,
                        "type": [
                            "string"
                        ]
                    },
                    "contact_id": {
                        "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                    },
                    "end_time": {
                        "format": "time",
                        "required": true,
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "events": {
                        "items": {
                            "extends": {
                                "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/versioned_entity_schema.json#"
                            },
                            "properties": {
                                "event_id": {
                                    "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                                },
                                "instruments": {
                                    "items": {
                                        "properties": {
                                            "instrument_id": {
                                                "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                                            },
                                            "instrument_template_id": {
                                                "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                                            },
                                            "name": {
                                                "required": true,
                                                "type": "string"
                                            },
                                            "response_set": {
                                                "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/response_set_schema.json#"
                                            }
                                        }
                                    },
                                    "minItems": 0,
                                    "required": true,
                                    "type": "array"
                                },
                                "name": {
                                    "required": true,
                                    "type": "string"
                                }
                            }
                        },
                        "minItems": 0,
                        "required": true,
                        "type": "array"
                    },
                    "person_id": {
                        "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                    },
                    "start_time": {
                        "format": "time",
                        "required": true,
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "type": {
                        "required": true,
                        "type": "integer"
                    }
                }
            },
            "required": true,
            "type": "array"
        },
        "participants": {
            "items": {
                "extends": {
                    "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/versioned_entity_schema.json#"
                },
                "properties": {
                    "p_id": {
                        "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                    },
                    "persons": {
                        "items": {
                            "extends": {
                                "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/versioned_entity_schema.json#"
                            },
                            "properties": {
                                "cell_phone": {
                                    "format": "phone",
                                    "type": "string"
                                },
                                "city": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string"
                                },
                                "home_phone": {
                                    "format": "phone",
                                    "type": "string"
                                },
                                "name": {
                                    "required": true,
                                    "type": "string"
                                },
                                "person_id": {
                                    "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/entity_id_schema.json#"
                                },
                                "relationship_code": {
                                    "required": true,
                                    "type": "integer"
                                },
                                "state": {
                                    "type": "string"
                                },
                                "street": {
                                    "type": "string"
                                },
                                "zip_code": {
                                    "type": "string"
                                }
                            }
                        },
                        "required": true,
                        "type": "array"
                    }
                }
            },
            "required": true,
            "type": "array"
        }
    }
}
