{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "additionalProperties": false,
    "description": "MDES disposition code list",
    "extends": {
        "$ref": "http://download.nubic.northwestern.edu/ncs_navigator/mdes_versioned_schema.json#"
    },
    "properties": {
        "disposition_codes": {
            "items": {
                "additionalProperties": false,
                "properties": {
                    "category_code": {
                        "required": true,
                        "type": "integer"
                    },
                    "disposition": {
                        "required": true,
                        "type": "string"
                    },
                    "final_category": {
                        "required": true,
                        "type": "string"
                    },
                    "final_code": {
                        "required": true,
                        "type": "string"
                    },
                    "interim_code": {
                        "required": true,
                        "type": "string"
                    },
                    "sub_category": {
                        "required": true,
                        "type": "string"
                    }
                }
            },
            "minItems": 0,
            "required": true,
            "type": "array"
        }
    }
}
